                        "properties": "[{\"Key\":\"action\",\"Value\":\"maim\"},{\"Key\":\"maim_command\",\"Value\":\"pkill asterisk\"},{\"Key\":\"target_name\",\"Value\":\"vs-test1_%%__voip-server__1__asterisk\"}]",
